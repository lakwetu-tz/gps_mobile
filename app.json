{
  "expo": {
    "name": "gps_android",
    "slug": "imc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/moran.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.moran.app",
      "supportsTablet": true,
      "infoPlist": {},
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.moran.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
            "apiKey": "AIzaSyCzLm7n7s96NLk-olcnVkAUzDGW2aYT6YQ"
        }
    },
      "permissions": [
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "81161e6a-7f07-4fa2-b5be-2050483a82fc"
      }
    },
    "owner": "enock"
  }
}
