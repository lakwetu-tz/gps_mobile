{
  "name": "gps_android",
  "version": "1.0.0",
  "private": true,
  "main": "src/app.tsx",
  "engines": {
    "node": "18",
    "npm": "9"
  },
  "engineStrict": true,
  "scripts": {
    "prepare": "husky install && cp .env.example .env",
    "start": "expo start",
    "start-tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "expo-build-ios": "eas build --platform ios",
    "expo-build-android": "eas build --platform android",
    "expo-build-all": "eas build --platform all",
    "expo-submit-ios": "eas submit --platform ios",
    "expo-submit-android": "eas submit --platform android",
    "expo-submit-all": "eas submit --platform all",
    "expo-build-ci-ios": "eas build --profile production --platform ios --auto-submit --non-interactive",
    "expo-build-ci-android": "eas build --profile production --platform android --auto-submit --non-interactive",
    "expo-build-ci-all": "eas build --profile production --platform all --auto-submit --non-interactive",
    "expo-dev-build-ios": "eas build --profile development --platform ios",
    "expo-dev-build-android": "eas build --profile development --platform android"
  },
  "prettier": {
    "useTabs": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@expo/metro-runtime": "^2.2.14",
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/netinfo": "9.3.10",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.25",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.7",
    "expo": "~49.0.15",
    "expo-device": "~5.4.0",
    "expo-image": "~1.3.5",
    "expo-notifications": "~0.20.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-flash-message": "^0.4.2",
    "react-native-maps": "1.7.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^8.1.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.4",
    "typescript": "^5.2.2",
    "react-native-gesture-handler": "~2.12.0",
    "expo-build-properties": "~0.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/lodash": "^4.14.201",
    "@types/redux-logger": "^3.0.12",
    "husky": "^8.0.3",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.1.0"
  }
}
